    <validationRules>
        <fullName>Check_custom_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND  (
ISPICKVAL(Ending_Period__c,&quot;Custom&quot;) ,
ISNULL(Custom_Ending_Period__c)
)</errorConditionFormula>
        <errorDisplayField>Custom_Ending_Period__c</errorDisplayField>
        <errorMessage>You must enter a Custom Ending Period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_date_saturday_check</fullName>
        <active>true</active>
        <errorConditionFormula>(MOD( Custom_Ending_Period__c  - DATE(1900, 1, 7), 7)=6) &amp;&amp;  (Include_Saturday__c!=true )</errorConditionFormula>
        <errorMessage>Your custom ending period is on a Saturday which you do not currently have selected as a work day.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_date_sunday_check</fullName>
        <active>true</active>
        <errorConditionFormula>(MOD( Custom_Ending_Period__c  - DATE(1900, 1, 7), 7)=0) &amp;&amp;  (Include_Sunday__c!=true )</errorConditionFormula>
        <errorMessage>Your custom ending period is on a Sunday which you do not currently have selected as a work day.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>date_check</fullName>
        <active>false</active>
        <errorConditionFormula>Custom_Ending_Period__c &lt; TODAY()</errorConditionFormula>
        <errorMessage>Date cannot be in the past.</errorMessage>
    </validationRules>
