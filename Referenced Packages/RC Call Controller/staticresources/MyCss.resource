.shadow {
	background: #D4D4D4 url(/resource/1308238753000/ring__spinner2) no-repeat 50% 32px scroll;
	opacity:0.5;
	filter: alpha(opacity=50);
	cursor:default;
	position:absolute;
	width:196px;
	height:100%;
	top:0;
	left:0;
	height:100%;
	display:none;
	z-index:99999;
}
.float-left {float: left;}
p.numberr {background: url(/resource/1290602106000/ring__img_call_n) no-repeat;padding:1px 0 10px 31px;}
p.last-call {
  background: url(/resource/1289552208000/ring__images) no-repeat 2px -30px;
  padding:1px 0 5px 21px;
}
.check {
  height: 16px;
  cursor: pointer;
  padding-left: 20px;
  background: url(/resource/1289552208000/ring__images) no-repeat 0px -665px; 
}
.check.c {background: url(/resource/1289552208000/ring__images) no-repeat 0px -685px; }
.check input {
  display: none;
}


.input {}
.input input {
border: 0;
background: 0;
width: 175px;
height: 21px;
padding: 2px 0;
font-weight: bold;
color: #4F5459;
font-size: 16px;
}
a.tab-header {
display:block;
text-decoration:none !important;
color:#307FAA !important;
font-weight:bold;
font-size:11px;
}
.closetab {
	cursor: pointer;
	display: none;
	margin-top: 20px;
	float: right;
	margin-right: 12px;
	* margin-bottom: -15px;
}
#last_close {
	* margin-bottom: -11px;
}
#closetab1, #closetab2, #closetab3 {
	cursor: pointer;
}
.callstab, .generaltab, .moretab, .logintab {
	border-bottom:1px solid #666;
	/*margin-bottom:10px;*/
	background:url(/resource/1286884522000/ring__arr_down) no-repeat 140px 0px;
}
.callstab.close, .generaltab.close, .moretab.close, .logintab.close {border-bottom:1px solid #F89820;/*margin-bottom:10px;*/background:0;}
.callstab.close .closetab, .generaltab.close .closetab, .moretab.close .closetab, .logintab.close .closetab {display:block;width:31px;height:14px;background:url(/resource/1286884483000/ring__arr_up) no-repeat;text-indent:-9999px;}

.tabs-body {display:none;}
.callstab.close .tabs-body, .generaltab.close .tabs-body, .moretab.close .tabs-body, .logintab.close .tabs-body {display:block;}


.haxe_wav, .haxe_pdf {position:absolute;height: 27px; width: 164px;float:left;left:-9999px;z-index:9;}
.haxe_pdf.show, .haxe_wav.show {position:relative;left:auto;}
.haxe_pdf object {
margin: 0 auto;
display: block;
}
.divtime {
	position: absolute;
	right: 0px;
	top: 0px;
}