    <webLinks>
        <fullName>Link_Existing_Smartsheet</fullName>
        <availability>online</availability>
        <description>Link an existing Smartsheet to an account or opportunity.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Link Existing Smartsheet</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>// Identify path components in URL (last one will also include query string)
var urlParts = top.location.href.split(&quot;/&quot;);

// Remove query string
urlParts = urlParts[urlParts.length - 1].split(&quot;?&quot;);
var recordId = urlParts[0];

// Navigate to SmartsheetLink page
top.location.href = &quot;/apex/com_smartsheet__SmartsheetLink?id=&quot; + recordId;

function urlEncode(str) {
    return escape(str).replace(&quot;+&quot;, &quot;%2B&quot;).replace(&quot;/&quot;, &quot;%2F&quot;);
}</url>
    </webLinks>
