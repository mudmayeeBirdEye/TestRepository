  <script>
 

  function LTrim( value ) {
                var re = /\s*((\S+\s*)*)/;
                return value.replace(re, "$1");
             }
            function RTrim( value ) {
                var re = /((\s*\S+)*)\s*/;
                return value.replace(re, "$1");
              }
            function trim( value ) {
                return RTrim(LTrim(value));
            }
            function suggest(text,Ereignis) {
                try {
                     if(Ereignis== null || Ereignis== undefined) {
                     Ereignis = window.event;
                     }
                    var enterKeyCode = -1;
                    if(Ereignis.keyCode != undefined && Ereignis.keyCode != null) {
                        enterKeyCode =  Ereignis.keyCode;
                    } else if(Ereignis.which != undefined && Ereignis.which != null ) {
                        enterKeyCode = Ereignis.which;
                    }
            if(enterKeyCode != 13){
                var html = '';
                var panelObj = document.getElementById('suggestionsList');
                var resultArr = new Array();
                 var iCountSize=0;
                if(panelObj != null) {
                    var searchText =  trim(text);
                     searchText = searchText.replace(/ {2,}/g,' ');
                    var breakIndex = 5;
                    if(searchText.length == 0) {
                        $('#suggestionsList').html('');
                         setTimeout("$('#suggestions').fadeOut();", 1); 
                    } else {
                         staticResourceArr.sort();
                        for(var i = 0; i < staticResourceArr.length; i++) {
                           if(staticResourceArr[i].toUpperCase().indexOf(trim(searchText.toUpperCase())) ==0) {
                                resultArr[resultArr.length] = i;
                                iCountSize++;
                                if(resultArr.length == breakIndex ) {
                                    break;
                                }
                            }
                        }
                         if(iCountSize<5){
                            resultArr=searchWordWise(searchText,resultArr,iCountSize);
                          }
                        if(resultArr.length == 0) {
                            $('#suggestionsList').html('');
                            setTimeout("$('#suggestions').fadeOut();", 1); 
                        } else {
                            html = "<ul>";
                            for(var j = 0; j < resultArr.length; j++) {
                                html +="<li onClick=\"fill('"+escape(staticResourceArr[resultArr[j]])+"','suggestions','suggestionsList');\">"+ staticResourceArr[resultArr[j]] + "</li>";
                            }
                            html += "</ul>";
                            //panelObj.innerHTML = html;
                            
                            if('{!isExternalPage}'=='true'){
                                  $('#suggestions').removeClass('suggestionsBox');
                                  $('#suggestions').addClass('suggestionsBox1');
                             }else{
                                    $('#suggestions').removeClass('suggestionsBox1');
                                    $('#suggestions').addClass('suggestionsBox');
                                  }  
                            
                             $('#suggestions').fadeIn();
                             $('#suggestionsList').html(html);
                              }
                        }
                    }
                }
                else{
                      if(enterKeyCode == 13 && document.activeElement.tagName=='INPUT'){
                          Search('searchText');
                         }
                        setTimeout("$('#suggestions').fadeOut();", 10); 
                    
                    }
                }catch(x){}
            }
  function searchWordWise(text,arrResult,count){
       
        var inputString= new Array();
          var resultArray= new Array();
            inputString=text.split(' ');
            resultArray=arrResult;
            var isExist='false';
            try{
                for(var k=0;k<inputString.length && count<5;k++){
                  isExist='false';
                  for(var ii=0;ii<staticResourceArr.length && count<5;ii++){
                       if(staticResourceArr[ii].toUpperCase().indexOf(trim(inputString[k].toUpperCase())) != -1) {
                           for(var j = 0; j < arrResult.length; j++) {
                              if(arrResult[j]==ii) {
                                 isExist=true;
                                 break;
                             }
                     }
                     if(isExist=='false'  ){
                           arrResult[arrResult.length] = ii;
                           count++;
                   } 
                   if(arrResult.length==5){
                      count=5;
                   }   
                 }
             
             }
         }
     }catch(x){
           //alert(x);
            return resultArray;
         }
         return arrResult;
     }
          


    function fill(thisValue,divId1,divId2) {
         if(thisValue!=''){
           $('#searchText').val(unescape(thisValue));
