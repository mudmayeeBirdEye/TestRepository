             if(getFilterDiv3.className == "showListButton"){
                twistSection(getFilterDiv3);
             }
             if(getFilterDiv4.className == "showListButton"){
                twistSection(getFilterDiv4);
             }
            }
        }
        function toogleFilterALL_customer(getType){
            var getFilterDiv1 = document.getElementById("img_{!$Component.pblk2.divAccountHealthSummay}");  
            var getFilterDiv2 = document.getElementById("img_{!$Component.pblk2.divNotesAndAttachments}"); 
            var getFilterDiv3 = document.getElementById("img_{!$Component.form1.PageBlock.Pgbnew}");  
            var getFilterDiv4 = document.getElementById("img_{!$Component.form1.PageBlock.PBS3}"); 
            if(getType == 0) {
             if(getFilterDiv1.className == "hideListButton"){
                twistSection(getFilterDiv1);
                $('.pbsub').hide();
             }
             if(getFilterDiv2.className == "hideListButton"){
