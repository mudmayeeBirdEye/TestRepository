        function switchPBS(currentElement, event) {
           var el = event.target || event.srcElement.tagName,
               topdiv = $(el).parents('.pbSubsection');
                       // alert($(topdiv).hasClass('pbSubsection'))
            //var a = $(event.target).find('h3') 
            if(!$(topdiv).hasClass('pbSubsection')){
                var imageVar = $(currentElement).find('img');
            	if($(currentElement).find('img').hasClass('hideListButton')) {
	                $(currentElement).children('.pbSubsection').hide();
	                $(currentElement).find('img').removeClass('hideListButton');
	                $(currentElement).find('img').addClass('showListButton');
	            } else if($(currentElement).find('img').hasClass('showListButton')) {
			                $(currentElement).children('.pbSubsection').show();
			                $(currentElement).find('img').removeClass('showListButton');
			                $(currentElement).find('img').addClass('hideListButton');
	            }
            } 
        }
            
        function hideUpsellDiv(divId) {
                var imageVar = $(divId).find('img');
                if($(divId).find('img').hasClass('hideListButton')) {
                    $('#UpsellOpportunitiesDiv').hide();
                    $(divId).find('img').removeClass('hideListButton');
                    $(divId).find('img').addClass('showListButton');
                } else if($(divId).find('img').hasClass('showListButton')) {
                            $('#UpsellOpportunitiesDiv').show();
                            $(divId).find('img').addClass('hideListButton');
                            $(divId).find('img').removeClass('showListButton');
                }
        }
        function hideUpsellDiv1(divId) {
                var imageVar = $(divId).find('img');
                if($(divId).find('img').hasClass('hideListButton')) {
                    $('#CrossSellOpportunitiesDiv').hide();
                    $(divId).find('img').removeClass('hideListButton');
                    $(divId).find('img').addClass('showListButton');
                } else if($(divId).find('img').hasClass('showListButton')) {
                            $('#CrossSellOpportunitiesDiv').show();
                            $(divId).find('img').addClass('hideListButton');
                            $(divId).find('img').removeClass('showListButton');
                }
        }
        $(document).ready(function() {
            $('#expand').click(function() {
                $('.ProfDescCls').show();
                $('.ProfDescCls').prev().find('img').removeClass('showListButton');
                $('.ProfDescCls').prev().find('img').addClass('hideListButton');
                $('.pbSubsection').show();
                $('.pbSubsection').prev().find('img').removeClass('showListButton');
                $('.pbSubsection').prev().find('img').addClass('hideListButton');
                $('.pbsub').show();
                 
            });
            $('#collaspse').click(function() {
                $('.ProfDescCls').hide();
                $('.ProfDescCls').prev().find('img').removeClass('hideListButton');
                $('.ProfDescCls').prev().find('img').addClass('showListButton');
                $('.pbSubsection').hide();
                $('.pbSubsection').prev().find('img').removeClass('hideListButton');
                $('.pbSubsection').prev().find('img').addClass('showListButton');
                $('.pbsub').hide();
                
            }); 
            
        });
        
        function switchPgBlock(currentElement) {
            //alert(1);
            var imageVar = $(currentElement).find('img');
            if($(currentElement).find('img').hasClass('hideListButton')) {
                $(currentElement).find('img').removeClass('hideListButton');
                $(currentElement).find('img').addClass('showListButton');
                $(currentElement).next().hide();
               // $(currentElement).find('img').removeClass('hideListButton');
              //  $(currentElement).find('img').addClass('showListButton');
            } else if($(currentElement).find('img').hasClass('showListButton')) {
                 $(currentElement).find('img').removeClass('showListButton');
               	 $(currentElement).find('img').addClass('hideListButton');
               	 $(currentElement).next().show();
               
               // $(currentElement).find('img').addClass('hideListButton');
               // $(currentElement).find('img').removeClass('showListButton');
            }
        }   
        
        function toogleFilterALL(getType) {
            var getFilterDiv1 = document.getElementById("img_{!$Component.pblk1.divCase}");  
            var getFilterDiv2 = document.getElementById("img_{!$Component.pblk1.divActivityHistories}");     
            var getFilterDiv3 = document.getElementById("img_{!$Component.pblk1.divContactRoles}"); 
            var getFilterDiv4 = document.getElementById("img_{!$Component.pblk1.divOpenActivities}");   
            if(getType == 0) {
             if(getFilterDiv1.className == "hideListButton"){
                twistSection(getFilterDiv1);
             }
             if(getFilterDiv2.className == "hideListButton"){
                 twistSection(getFilterDiv2);
             }
             if(getFilterDiv3.className == "hideListButton"){
                twistSection(getFilterDiv3);
             }
             if(getFilterDiv4.className == "hideListButton"){
                twistSection(getFilterDiv4);
             }
            }else {
           
