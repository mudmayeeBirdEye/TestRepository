<script>
$(document).ready(function () {
    $("#helpDesk").click(function () {
        if($(".subNav").is(":hidden")==true){
        	$(".subNav").show();
        	$(this).addClass("active");
        	$(".subNavs").hide();
        	$("#external").removeClass("active");
        	$(".subNaval").hide();     
        	$("#global").removeClass("active");
        } else {
        	$(".subNav").hide();
        	$(this).removeClass("active");
        }
    });
});
$(document).ready(function () {
    $("#external").click(function () {
        if($(".subNavs").is(":hidden")==true){
        	$(".subNavs").show();
        	$(this).addClass("active");
        	$(".subNaval").hide();     
        	$("#global").removeClass("active");
        	$(".subNav").hide();
        	$("#helpDesk").removeClass("active");     
        }else{
        	$(".subNavs").hide();
        	$(this).removeClass("active");
        }
    });
});

$(document).ready(function () {
    $("#global").click(function () {
        if($(".subNaval").is(":hidden")==true){
        	$(".subNaval").show();
        	$(this).addClass("active"); 
        	$(".subNav").hide();
        	$("#helpDesk").removeClass("active");
        	$(".subNavs").hide();
        	$("#external").removeClass("active");    
        }else{
        	$(".subNaval").hide();
        	$(this).removeClass("active");
        }
    });
})

</script>
