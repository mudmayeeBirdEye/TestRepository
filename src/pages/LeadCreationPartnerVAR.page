        function confirmCancel() {
            var isCancel = confirm("Are you sure you wish to cancel?");
            if (isCancel) {
                return true;
            } 
            return false; 
        } 
    
        function formatNumber(phoneNum) {
            var sVal = '';
            var oldValue;
            var nVal = phoneNum.length;
            var sChar = '';
            oldValue = phoneNum;
        
            try {
                for (c = 0; c < nVal; c++) {
                    sChar = phoneNum.charAt(c);
                    sChar = phoneNum.charAt(c);
                    nChar = sChar.charCodeAt(0);
        
                    if ((nChar >= 48) && (nChar <= 57)) {
                        sVal += phoneNum.charAt(c);
                    }
                }
            } catch (exception) { //AlertError("Format Number",exception); 
                sVal = oldValue;
            }
            /*if (sVal.length > 10) {
                try {
                    oldValue = sVal;
                    sVal = sVal.substring((sVal.length - 10));
                } catch (exception) {
                    sVal = oldValue;
                }
            }*/
            return sVal;
        }
        
        $(document).ready(function() {
            $(".defaultText").click(function(srcc) {
                if ($(this).val() == $(this)[0].title) {
                    $(this).removeClass("defaultTextActive");
                    $(this).val("");
                }
            });
            $(".defaultText").focus(function(srcc) {
                if ($(this).val() == $(this)[0].title) {
                    if($(this)[0].title != 'First Name') {
                        $(this).removeClass("defaultTextActive");
                        // alert($(this)[0].title);
                        $(this).val("");
                    }
                }
            });
            $(".defaultText").blur(function() {
                if ($(this).val() == "") {
                    $(this).addClass("defaultTextActive");
                    $(this).val($(this)[0].title);
                } else if($(this).val() != "" ) {
                    if($(this)[0].title == "Contact Phone Number" && $(this).val() != "Contact Phone Number") {
                        var phoneVal = FormatNumber($(this).val());
                        $(this).val(phoneVal);
                    } else if($(this).val() == $(this)[0].title){
                        $(this).addClass("defaultTextActive");
                        $(this).val($(this)[0].title);
                    }
                }
            });
            $(".defaultText").blur();        
        });
        
        $(document).ready(function() {
            if ($.browser.mozilla) {
                 $('textarea[rows]').each(function(i, el) {
                     if (!$(el).data('ffRowsFixed')) {
                         var rows = parseInt($(el).attr('rows'));
                         if (rows > 1) {
                             $(el).attr('rows', (rows - 1));
                         }
                         $(el).data('ffRowsFixed', true);
                     }
                 });
            }
        });
        
        function checkLength(control, maximum, e) {
            var keycode;
            if (window.event) keycode = window.event.keyCode;
            else if (e) keycode = e.which;
            if(keycode != 8) {
                var length = control.value.length;
                if (e.keyCode == 32 && (length >= maximum)) {
                    e.preventDefault();
                } else {
                    if (length >= maximum) {
                        alert('Please limit your value to '+ maximum + ' characters. There are currently ' + length + ' characters.');
                        return false;
                    }
                    return true;
                } 
            } 
        }
        
        function fnClaimOwnership(clickedField) {
            var claimedLeadId = clickedField.value;
             //alert(clickedField.id);
             //alert(document.getElementById(clickedField.id).checked);
            document.getElementById(clickedField.id).checked=false;;
            if (claimedLeadId) {
                $("#confirmationDivYes").attr('data-id', claimedLeadId);
                $("#confirmationDiv").dialog({
                     height : 140,
                     width : 700,
                     modal : true
                });
                $(".ui-widget-overlay").css("opacity","0.87");
            } else {                
                return false;
            }
        }
        function processClaim() {
           var claimedLeadId = $("#confirmationDivYes").attr('data-id');
           $("#claimedLeadIdInput").val(claimedLeadId);
           $("#confirmationDiv").dialog('close');
           showOverlay();
           claimLeadOwnership();
        }
    
        function stopClaim() {
            $("#confirmationDiv").dialog('close');
        }
        
        
        
    </script>
