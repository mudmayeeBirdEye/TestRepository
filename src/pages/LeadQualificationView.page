            $("table tr td input[name=LQSelection]:radio").click(function(srcc) {
                try {
                    if(toshowViewMode == 'false') {
                        
                    } else {
                        $(".inputselect").closest('tr').css('background-color', 'transparent'); 
                        $(this).closest('tr').css('background-color', '#F0F0F0');
                    }
                } catch(e) {alert(e);} 
            });
        });
        function selectRow() {
            var selectedVal = '{!selectedLeadQual}';
            $("table tr td input[name=LQSelection]:radio").each(function() {
                if(selectedVal != "" && $(this).val() != "") {
                    if(selectedVal == $(this).val()) {
                        prevselected = $(this).val();
                        $(this).attr("checked",true);
                        $(this).closest('tr').css('background', '#F0F0F0');
                    }
                }
            });
        }
        selectRow(); 
        
        function loadLeadQualDet(id) {
            if(toshowViewMode == 'false') {
               
                var isConfirm = confirm('Do you really want to continue without saving changes?');
                if(isConfirm) {
                    toshowViewMode = 'true';
                    prevselected = $("table tr td input[name=LQSelection]:radio:checked").val();
                    loadLeadQualDet1(id);
                } else {
                   document.getElementById(prevselected).checked = true;   
                }
            } else {
                prevselected = $("table tr td input[name=LQSelection]:radio:checked").val();
                toshowViewMode = 'true'; 
                loadLeadQualDet1(id);
            }
        }
        
        function backClorRowIE() {
            //alert(1);
            $(".inputselect").closest('tr').css('background-color', 'transparent');
            var selRow = 'tr'+ prevselected; 
            document.getElementById(selRow).style.background = '#F0F0F0';
        }
        
        function EditMode() {
            toshowViewMode = 'false';
            EditMode1();
        }
        
        function SaveLeadQ() {
            toshowViewMode = 'true';
            SaveLeadQ1();
        }
        
        function CancelLeadQual() {
            if(toshowViewMode == 'false') {
                var isConfirm = confirm('Do you really want to continue without saving changes?'); 
                if(isConfirm) {
                    toshowViewMode = 'true'; 
                    CancelLeadQual1();
                }
            } else {
                toshowViewMode = 'true'; 
                CancelLeadQual1();
            }
        }
       
