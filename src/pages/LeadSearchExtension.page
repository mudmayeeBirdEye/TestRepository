    function showDecMaker(maincls,hiddenpanel) {
        var display = 'none';
        if($('select.'+maincls+' option:selected').val() == 'No') {
            display = 'block';
        } 
        $('.'+hiddenpanel).each(function() {
            $(this).css('display',display);
        });
    }
    
    function checkLength(control, maximum, e) {
        var keycode;
        if (window.event) keycode = window.event.keyCode;
        else if (e) keycode = e.which;
        if(keycode != 8) {
            var length = control.value.length;
            if (e.keyCode == 32 && (length >= maximum)) {
                e.preventDefault();
            } else {
                if (length >= maximum) {
                    alert('Please limit your value to '+ maximum + ' characters. There are currently ' + length + ' characters.');
                    return false;
                }
                return true;
            } 
        } 
    }
    
