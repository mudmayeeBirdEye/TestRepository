<script>
    
    //-------------------------------------------Implementation details Javascript starts------------------------------------

    //-----------------------getting all Call flow models and features dependency json from controller-------------
    var controllerList  = jQuery.parseJSON( '{!getDependencyList}');
    //------------------------------function to create dependency and binding event on page level--------------------
    function createDependency(modelValue){
        $('.jsDependentClass').find('select,input,textarea').prop('disabled',false);
        var jsonObj = [];
        var itemObj = {};
        $('.jsDependentClass input:checkbox').each(function() {
            var id = $(this).attr("id");
            var value = $(this).val();
            itemObj = {};
            itemObj['key'] = value;
            itemObj['id'] = id;
            jsonObj.push(itemObj);
        });
        var childArray =  (controllerList[modelValue]).childList;
        for(j in jsonObj){
            $( document.getElementById(jsonObj[j].id) ).prop( "checked", false );
            for(var i in childArray){
                if(jsonObj[j].key == childArray[i]){ 
                    $( document.getElementById(jsonObj[j].id) ).prop( "checked", true );
                 }   
            } 
        }
    }
    //-------------------------------------------Implementation details Javascript ends------------------------------------    

    //-------------------------------------------Discovery guide related javascript starts---------------------------------     
      
      //--------------------------------------Function to initiate tab nature------------------------------------------- 
      function initiateTabs(tabWrapper,options){
        var optionsJson = jQuery.parseJSON(options);
        if(optionsJson.activeTab != null){
            $(tabWrapper).children('.tabdiv').hide().eq(optionsJson.activeTab).show();
        }
        else{
            $(tabWrapper).children('.tabdiv').hide().eq(0).show();
        }
      };
      
      //-------------------------------------Function to check and show errors in discovery tab-------------------------   
      function CheckErrorTabs(){
        $.each($(".box-wrap").children(".tabdiv"),function(){
             var parent = $(this);
            $(this).find(".errorMsg").filter(function(){
                     
                    $(".box-wrap").siblings("ul").find("li[id*="+parent.attr('id')+"]").each(function(){
                         $(this).addClass("errorTab");
                    //     alert($(this).attr("id"));
                       
                    });
                });
        });
      };
     
